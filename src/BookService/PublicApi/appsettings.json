{
  "CatalogBaseUrl": "",
  "TrackUrlTemplate": "api/books/{0}/tracks/{1}/{2}",
  "TracksFolder": "~/content/tracks",
  "TempTracksFolder": "~/content/tempTracks",
  "JWTToken": {
    "AccessTokenExpirationMinutes": 15,
    "RefreshTokenExpirationDays": 30
  },
  "PushNotification": {
    "BaseAddress": "https://api.push.apple.com/3/",
    "Duration": "00:30"
  },
  "AppStoreSubscription": {
    "BuyAddress": "https://buy.itunes.apple.com/",
    "SandboxAddress": "https://sandbox.itunes.apple.com/"
  },
  "UseSwagger": false,
  "UserFreeBookLimit": {
    "FromRegisterUserDate": "2021-03-23",
    "Limit": 5
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}